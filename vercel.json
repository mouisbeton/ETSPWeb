{
  "framework": "laravel",
  "buildCommand": "npm install && npm run build && php artisan migrate --force",
  "outputDirectory": "public",
  "env": {
    "APP_NAME": "Task Manager",
    "APP_ENV": "@APP_ENV",
    "APP_KEY": "@APP_KEY",
    "APP_DEBUG": "@APP_DEBUG",
    "APP_URL": "@APP_URL",
    "DB_CONNECTION": "@DB_CONNECTION",
    "DB_HOST": "@DB_HOST",
    "DB_PORT": "@DB_PORT",
    "DB_DATABASE": "@DB_DATABASE",
    "DB_USERNAME": "@DB_USERNAME",
    "DB_PASSWORD": "@DB_PASSWORD",
    "CACHE_STORE": "file",
    "SESSION_DRIVER": "file",
    "QUEUE_CONNECTION": "sync"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php",
      "methods": ["GET", "HEAD", "PUT", "POST", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
